<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="cordova-plugin-analytics-mixpanelanalytics"
    version="1.0.40">
	
    <name>Cordova MixpanelAnalytics plugin</name>
    <description>Cordova MixpanelAnalytics plugin</description>
    <author>Sang Ki Kwon (Cranberrygame)</author>		
    <license>MIT</license>
    <keywords>cordova,plugin,analytics,mixpanelanalytics</keywords>
    <repo>https://github.com/cranberrygame/cordova-plugin-analytics-mixpanelanalytics</repo>
    <issue>https://github.com/cranberrygame/cordova-plugin-analytics-mixpanelanalytics/issues</issue> 	
	<engines>
	    <engine name="cordova" version=">=3.0.0" />
	</engines>	
		
    <js-module src="www/mixpanelanalytics.js" name="mixpanelanalytics">
        <clobbers target="window.mixpanelanalytics"/>
    </js-module>

    <platform name="android">
        <!-- config.xml -->
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="MixpanelAnalytics">
                <param name="android-package" value="com.cranberrygame.cordova.plugin.mixpanelanalytics.MixpanelPlugin"/>
            </feature>
        </config-file>

        <!-- Plugin files -->
        <source-file src="src/android/MixpanelPlugin.java" target-dir="src/com/cranberrygame/cordova/plugin/mixpanelanalytics"/>
        <source-file src="src/android/mixpanel-android-4.5.3.jar" target-dir="libs/"/>

        <!-- manifest -->
        <!-- see https://github.com/mixpanel/mixpanel-android/blob/master/src/main/AndroidManifest.xml -->
        <config-file target="AndroidManifest.xml" parent="/manifest">
            <uses-sdk android:minSdkVersion="9"/>
            <uses-permission android:name="android.permission.INTERNET"/>
            <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
        </config-file>
    </platform>

    <platform name="ios">
        <!-- config.xml -->
        <config-file target="config.xml" parent="/*">
            <feature name="MixpanelAnalytics">
                <param name="ios-package" value="MixpanelPlugin"/>
            </feature>
        </config-file>

        <!-- Plugin files-->
        <header-file src="src/ios/MixpanelPlugin.h"/>
        <source-file src="src/ios/MixpanelPlugin.m"/>

